%h2 Please complete your profile

= form_for @people, url: createandlogin_path,html: { multipart: true} do |form|
  
   = form.label :people, :username
   = form.text_field :username
  
   = form.label :people, :description
   = form.text_field :description

   = form.label :people, :company
   = form.text_field :company

   = form.label :people, :start_date
   = form.text_field :start_date

   = form.label :people, :resume
   = form.text_field :resume

   = form.label :people, :university
   = form.text_field :university

   = form.label :people, :major
   = form.text_field :major

   = form.label :people, :graduation
   = form.text_field :graduation

   = form.label :people, :occupation
   = form.select(:occupation, options_for_select([['Software Engineering'],['CEO'],['COO'],['MD'],['CFO'],['CTO']]))

   = form.label :people, :avatar
   = form.file_field :avatar
   = form.submit 'Submit Profile'
/ = form_tag createandlogin_path do

/   = cookies[:email].inspect

/   = label :people, :username, 'Username'
/   = text_field :people, 'username'
/   = label :people, :description, 'Description'
/   = text_field :people, 'description'
/   = label :people, :company, 'Company'
/   = text_field :people, 'company'
/   = label :people, :start_date, 'Start Date'
/   = text_field :people, 'start_date'
/   = label :people, :resume, 'Resume'
/   = text_field :people, 'resume'
/   = label :people, :university, 'University'
/   = text_field :people, 'university'
/   = label :people, :major, 'Major'
/   = text_field :people, 'major'
/   = label :people, :graduation, 'Graduation Year'
/   = text_field :people, 'graduation'
/   = submit_tag 'Submit Profile'
